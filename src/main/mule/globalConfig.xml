		<http:listener-connection host="${http.host}" port="${http.port}" />
	</http:listener-config>
	<configuration-properties doc:name="Configuration properties" doc:id="1f7d191b-397c-406c-8f9a-d60a8a1019b3" file="${env}-properties.yaml" />
	<global-property doc:name="Global Property" doc:id="2b8c135e-f78b-4294-b6df-c9a89ee6df4c" name="env" value="dev" />
</mule>
